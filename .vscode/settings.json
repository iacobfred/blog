{
  "auto-rename-tag.activationOnLanguage": ["html", "*"],
  "auto-close-tag.activationOnLanguage": ["html"],
  "colorize.languages": ["javascript", "typescript", "typescriptreact", "css"],
  "debug.console.fontSize": 11,
  "debug.javascript.unmapMissingSources": true,
  "editor.codeActionsOnSave": {
    "source.organizeImports": "never",
    "source.fixAll": "explicit"
  },
  "editor.detectIndentation": true,
  "editor.formatOnSave": true,
  "editor.linkedEditing": true,
  "editor.rulers": [100],
  "editor.suggestSelection": "first",
  "eslint.experimental.useFlatConfig": true,
  "eslint.lintTask.enable": false,
  "eslint.format.enable": true,
  "explorer.autoReveal": false,
  "explorer.confirmDragAndDrop": false,
  "files.associations": {
    "**/*.html": "html",
    "**/*.htm": "html"
  },
  "files.autoSave": "onFocusChange",
  "files.autoSaveDelay": 3000,
  "files.eol": "\n",
  "files.exclude": {
    "**/.git": true,
    "**/.svn": true,
    "**/.hg": true,
    "**/CVS": true,
    "**/.DS_Store": true,
    "**/Thumbs.db": true,
    "**/*.pyc": true,
    "**/__pycache__": true,
    "**/.next": false,
    "**/node_modules": true,
    "dist": true
  },
  "gitlens.currentLine.scrollable": false,
  "html.autoClosingTags": true,
  "i18n-ally.localesPaths": ["./i18n/locales"],
  "i18n-ally.keystyle": "nested",
  "javascript.autoClosingTags": true,
  "php.suggest.basic": false,
  "php.validate.enable": false,
  "search.exclude": {
    "**/.next": true,
    "**/node_modules": true
  },
  "terminal.integrated.cursorWidth": 4,
  "terminal.integrated.cursorStyle": "line",
  "typescript.autoClosingTags": true,
  "workbench.tree.indent": 16,
  "workbench.iconTheme": "material-icon-theme",
  "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
  "workbench.editor.limit.enabled": true,
  "workbench.editor.limit.value": 7,
  "shellcheck.ignorePatterns": {
    "*.*ignore": true
  },
  "editor.tabSize": 2
}
