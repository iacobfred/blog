{
  "extends": "nx/presets/npm.json",
  "tasksRunnerOptions": {
    "default": {
      "runner": "nx/tasks-runners/default",
      "options": {
        "cacheableOperations": [
          "build",
          "dev",
          "lint",
          "test",
          "analyze",
          "cypress:open",
          "db:seed",
          "test:ci",
          "test:coverage",
          "test:coverage:view",
          "test:verbose",
          "test:watch",
          "test:watchAll"
        ]
      }
    }
  },
  "targetDefaults": {
    "build": {
      "dependsOn": [
        "^build"
      ]
    }
  },
  "affected": {
    "defaultBase": "main"
  }
}
