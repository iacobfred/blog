{
  "name": "self-builder",
  "version": "0.0.0",
  "keywords": [],
  "author": "Jacob Fredericksen <jacob.t.fredericksen@gmail.com>",
  "engines": {
    "node": "16.x",
    "npm": ">=8.0"
  },
  "os": [
    "darwin",
    "linux"
  ],
  "private": true,
  "scripts": {
    "analyze": "ANALYZE=true npm run build",
    "build": "next build",
    "code:generate": "npm run schema:generate",
    "cypress:open": "cypress open",
    "db:seed": "ts-node -r tsconfig-paths/register --type-check --compiler-options {\\\"module\\\":\\\"CommonJS\\\"} scripts/seed.ts",
    "dev": "concurrently \"npm run generate:watch\" \"npm run start:dev\"",
    "generate": "npm run schema:generate && graphql-codegen --config codegen.yml; eslint . --fix",
    "generate:watch": "graphql-codegen --config codegen.yml --watch",
    "lint": "next lint",
    "prepare": "npx husky install",
    "schema:generate": "for filename in graphql/generated/**/index.ts; do rm $filename; touch $filename; done; for filename in graphql/schema/definitions/*.ts; do node -r tsconfig-paths/register ./node_modules/.bin/hygen schema new --definition $(basename -- $filename); done",
    "start": "next start",
    "start:dev": "next dev",
    "test": "jest",
    "test:ci": "jest --ci",
    "test:coverage": "jest --coverage",
    "test:coverage:view": "jest --coverage && open coverage/lcov-report/index.html",
    "test:verbose": "jest --verbose",
    "test:watch": "jest --watch",
    "test:watchAll": "jest --watchAll"
  },
