    "//": "https://nx.dev/core-features/automate-updating-dependencies",
    "migrations:make": "nx migrate",
    "migrations:run": "nx migrate --run-migrations",
    "cypress:open": "cypress open",
    "db:seed": "ts-node --project tsconfig.base.json -r tsconfig-paths/register --type-check --compiler-options {\\\"module\\\":\\\"CommonJS\\\"} .config/scripts/seed.ts",
    "generate": "npm run code:generate",
    "prepare": "npx husky install",
    "code:generate": "export TS_NODE_PROJECT=tsconfig.base.json; for filename in **/generated/**/index.ts; do rm $filename; touch $filename; done; for filename in apps/web/_definitions/*.ts; do node -r tsconfig-paths/register ./node_modules/.bin/hygen _ new --definition $(basename -- $filename); done; prettier -w .; eslint . --fix"
  },
